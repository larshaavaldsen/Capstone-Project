html
  include header.pug
      
.card-group
.row.mb-5
  .col-md-8.col-xl-8.text-center.mx-auto
    .table-responsive
      table.table
        thead
          tr
            th Challenge
            th Status
            th Docs
        tbody
          each chall in statuses
            tr 
              th= chall.name  
              th= chall.completed
              td
                a(href=chall.docs) Link
script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js')
